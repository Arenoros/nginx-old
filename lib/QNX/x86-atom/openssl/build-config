add to 10-main.conf
	"QNX6-IA" => {
        inherit_from     => [ "BASE_unix" ],
        cc               => "i486-pc-nto-qnx6.5.0-gcc",
        cflags           => "-D__unix -DL_ENDIAN -march=atom -mtune=atom -O3 -flto -ffast-math -fno-use-linker-plugin",
        ex_libs          => add("-lsocket"),
        dso_scheme       => "dlfcn",
        shared_target    => "bsd-gcc-shared",
        shared_cflag     => "-fPIC",
        shared_extension => ".so.\$(SHLIB_MAJOR).\$(SHLIB_MINOR)",
    },

-----
./Configure QNX6-IA no-shared no-threads no-dso no-hw no-asm --prefix=/home/dan/nginx/lib/QNX/x86-atom/openssl

